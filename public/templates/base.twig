<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Yet Another Chat</title>
        <link rel="stylesheet" href="/css/global" type="text/css">
        {% block css %}{% endblock %}
    </head>
    <body>
        <div id="app">
            {% block content %}{% endblock %}
            {% block worms %}
                <div id="worms">
                    <div id="worms-left">
                        <div class="worm-container worm-1-container">
                            <svg class="worm worm-1">
                                <circle r="50%" cx="50%" cy="50%"></circle>
                            </svg>
                        </div>
                        <div class="worm-container worm-2-container">
                            <svg class="worm worm-2">
                                <circle r="50%" cx="50%" cy="50%"></circle>
                            </svg>
                        </div>
                        <div class="worm-container worm-3-container">
                            <svg class="worm worm-3">
                                <circle r="50%" cx="50%" cy="50%"></circle>
                            </svg>
                        </div>
                        <div class="worm-container worm-4-container">
                            <svg class="worm worm-4">
                                <circle r="50%" cx="50%" cy="50%"></circle>
                            </svg>
                        </div>
                        <div class="worm-container worm-5-container">
                            <svg class="worm worm-5">
                                <circle r="50%" cx="50%" cy="50%"></circle>
                            </svg>
                        </div>
                    </div>
                    <div id="worms-right">
                        <div class="worm-container worm-1-container">
                            <svg class="worm worm-1">
                                <circle r="50%" cx="50%" cy="50%"></circle>
                            </svg>
                        </div>
                        <div class="worm-container worm-2-container">
                            <svg class="worm worm-2">
                                <circle r="50%" cx="50%" cy="50%"></circle>
                            </svg>
                        </div>
                        <div class="worm-container worm-3-container">
                            <svg class="worm worm-3">
                                <circle r="50%" cx="50%" cy="50%"></circle>
                            </svg>
                        </div>
                        <div class="worm-container worm-4-container">
                            <svg class="worm worm-4">
                                <circle r="50%" cx="50%" cy="50%"></circle>
                            </svg>
                        </div>
                        <div class="worm-container worm-5-container">
                            <svg class="worm worm-5">
                                <circle r="50%" cx="50%" cy="50%"></circle>
                            </svg>
                        </div>
                    </div>
                </div>
            {% endblock %}
        </div>

        <script
			  src="https://code.jquery.com/jquery-3.5.1.min.js"
			  integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
              crossorigin="anonymous">
        </script>
        <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
        <script src="/js/global" type="module"></script>
        {% block js %}{% endblock %}
    </body>
</html>